{
  "name": "@launch-list/nextjs",
  "version": "0.1.1",
  "license": "MIT",
  "description": "LaunchList package for Next.js",
  "exports": {
    "./package.json": "./package.json",
    "./styles.css": "./dist/index.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@launch-list/tsconfig": "workspace:^",
    "@launch-list/tsup": "workspace:^",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "axios": "^1.5.0",
    "nextjs": "^0.0.3",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "tailwindcss": "3.3.3",
    "tsup": "^7.2.0",
    "typescript": "5.1.6",
    "zod": "3.21.4"
  },
  "dependencies": {
    "react-hook-form": "^7.45.4"
  }
}